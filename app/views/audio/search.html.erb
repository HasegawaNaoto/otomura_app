<div style="padding-top:100px;">
  <% @audios.each do |audio| %>
  <% audio_user=User.find_by(id: audio.user_id) %>
  <div style="margin-top:20px;">
    ユーザ名:<%= link_to(audio_user.name,"/users/userprofile/#{audio_user.public_uid}") %><br>
    タイトル:<%= link_to(audio.title,"/audio/show/#{audio.id}") %><br>
    ジャンル:
    <% @tags=AudioTag.where(id: audio.audio_tags.ids) %>
    <% @tags.each do |tag| %>
    <div class="userprofile-genre-btn">
      <%= link_to(tag.genre,"/audio/tag/#{tag.genre}") %>
    </div>
    <% end %>
  </div>
  <% end %>
</div>

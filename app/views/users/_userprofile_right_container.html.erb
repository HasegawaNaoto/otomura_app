<div class="userprofile-right-containers">
  <div class="userprofile-right-container">
    <div class="userprofile-userlist-header">
      <h3>ユーザ一覧</h3>
    </div>

    <div class="userprofile-userlist-content-container">
      <% @serch_users_load_33_or.first(10).each do |recomended_user| %>
      <% unless Friendlist.find_by(friend_id:recomended_user.id,user_id:@current_user.id) || recomended_user.id==@current_user.id || Follow.find_by(follow_id:recomended_user.id,user_id:@current_user.id) %>
      <%= render partial: 'posts/recomended_user_reply_btn',locals: {recomended_user:recomended_user}%>
      <% end %>
      <% end %>
    </div>
  </div>

  <div class="show-profile-koukoku-container">
    <%= image_tag 'test_koukoku.png' %>
  </div>
</div>
